1. Products

2.SELECT ProductID, ProductName, UnitPrice
FROM Products

3. SELECT ProductID, ProductName, UnitPrice
FROM Products
ORDER BY UnitPrice

4. SELECT ProductID, ProductName, UnitPrice
FROM Products
WHERE UnitPrice <= 7.5

5. SELECT ProductID, ProductName, UnitsInStock, UnitPrice
FROM Products
WHERE UnitsInStock >= 100
ORDER BY UnitPrice DESC

6. SELECT ProductID, ProductName, UnitsInStock, UnitPrice
FROM Products
WHERE UnitsInStock >= 100
ORDER BY UnitPrice DESC, ProductName
